<Window x:Class="WPFproject2.AlienWar"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFproject2"
        mc:Ignorable="d"
        Loaded="Window_Loaded"
        Closing="Window_Closing"
        KeyDown="Window_KeyDown"
        KeyUp="Window_KeyUp"
        Title="AlienWar" Height="600" Width="950">
    <Grid>
        <Canvas x:Name="GameField">
            <Image Source="{DynamicResource Space}" 
                   Width="950"
                   />
            <Image Source="{DynamicResource Ship}"
                   Width="100"
                   Canvas.Left ="400"
                   Canvas.Top="502"
                   x:Name="Ship"/>
            <!--<Image Source="{DynamicResource Alien2}" 
                    Width="100"
                   Canvas.Left ="400"
                   Canvas.Top="35"
                   x:Name="Alien"/>-->





        </Canvas>
    </Grid>
</Window>
